[sub_resource type="GDScript" id="GDScript_cpvyx"]
script/source = "extends ParallaxLayer

@export var speed: float = 0.0

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	motion_offset.x -= delta*speed
"

[sub_resource type="GDScript" id="GDScript_rpm3b"]
script/source = "extends ParallaxLayer

@export var player: Node2D 
@export var repeat_start: float = 200
@export var repeat_distance: float = 500
var repeat_end: float = INF
func _process(delta):
	if not player:
		return 
	
	var player_pos = player.global_position.x
	
	if player_pos < repeat_start or player_pos > repeat_end:
		motion_mirroring = Vector2.ZERO
	else:
		motion_mirroring = Vector2(repeat_distance, 0)
		
"

